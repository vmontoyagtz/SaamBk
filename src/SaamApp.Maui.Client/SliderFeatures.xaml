<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" 
             xmlns:sliders="clr-namespace:Syncfusion.Maui.Sliders;assembly=Syncfusion.Maui.Sliders" Title="Slider"
            x:Class="SaamApp.Maui.Client.SliderFeatures">
    <sliders:SfSlider Minimum="0"
                      Maximum="100"
                      Value="50"
                      ShowTicks="True"
                      ShowLabels="True">
        <sliders:SfSlider.Tooltip>
            <sliders:SliderTooltip />
        </sliders:SfSlider.Tooltip>
    </sliders:SfSlider>
</ContentPage>
