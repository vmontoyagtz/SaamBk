<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:SaamApp.Maui.Client"      
            xmlns:editors="clr-namespace:Syncfusion.Maui.Inputs;assembly=Syncfusion.Maui.Inputs"
             x:Class="SaamApp.Maui.Client.AutocompleteFeatures" Title="Autocomplete">
    <ContentPage.BindingContext>
        <local:SocialMediaViewModel />
    </ContentPage.BindingContext>
    <editors:SfAutocomplete IsClearButtonVisible="True" HeightRequest="50" WidthRequest="200" ItemsSource="{Binding SocialMedias}" TextSearchMode="StartsWith" DisplayMemberPath="Name">
    </editors:SfAutocomplete>
</ContentPage>
